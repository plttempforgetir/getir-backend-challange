{
  "name": "getir-backend-challenge",
  "version": "1.0.0",
  "description": "Getir - Backend Challenge",
  "main": "./src/index.js",
  "scripts": {
    "start": "node ./src/index.js"
  },
  "engines": {
    "node": ">=12.18.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es6": true,
      "node": true
    },
    "plugins": [
      "prettier"
    ],
    "extends": [
      "plugin:prettier/recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018
    }
  },
  "eslintIgnore": [
    "node_modules/*"
  ],
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bluebird": "^3.5.2",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validation": "^3.0.5",
    "mongoose": "^5.9.16"
  },
  "devDependencies": {
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier-eslint": "^8.8.2"
  }
}
